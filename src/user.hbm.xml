<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="classes.model.User" table="user_" lazy="false">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="id" type="java.lang.Integer" column="user_id"/>
        <property name="name" column="user_name" type="java.lang.String"/>
        <property name="surname" column="user_surname" type="java.lang.String"/>
        <property name="email" column="email" type="java.lang.String"/>
        <property name="phone" column="phone" type="java.lang.String"/>
        <property name="address" column="address" type="java.lang.String"/>
        <property name="login" column="login" type="java.lang.String"/>
        <property name="password" column="password" type="java.lang.String"/>
        <property name="version" column="version" type="java.lang.Integer"/>
        <property name="privilege" column="privilege" type="java.lang.String"/>
    </class>

</hibernate-mapping>